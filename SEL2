/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN in, sel;
    OUT out1, out2;

    PARTS:
    Not(in=sel , out=nsel );
    And(a=nsel , b=in , out=out1 );
    And(a=sel , b=in , out=out2 );
}
