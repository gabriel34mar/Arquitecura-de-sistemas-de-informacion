/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN x, y,z;
    OUT equal;

    PARTS:
    //// Replace this comment with your code.
Not(in=x , out=nx );
Not(in=y , out=ny );
And(a=nx , b=ny , out=equal );
}
